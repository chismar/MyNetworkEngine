                "$type": "DoAll",
                "CustomName":"Develop self",
                "Impacts": [
                    {
                        "$type": "ChangeEntityStat",
                        "Change": 1,
                        "StatRef": "/Stats/Prosperity"
                    },
                    {
                        "$type": "TargetEnvironment",
                        "Impact": {
                            "$type": "ChangeEntityStat",
                            "Change": -1,
                            "StatRef": "/Stats/Habitability"
                        }
                    }
                ]
            }
        },
        "Protect": {
            "$type": "DomainDef",
            "ImpactPerValue": {
                "$type": "ChangeEntityStat",
                "CustomName":"Protect",
                "Change": 1,
                "StatRef": "/Stats/Defence"
            }
        },
        "Clear": {
            "$type": "DomainDef",
            "ImpactPerValue": {
                "$type": "TargetEnvironment",
                "CustomName":"Clean",
                "Impact": {
                    "$type": "ChangeEntityStat",
                    "Change": 1,
                    "StatRef": "/Stats/Habitability"
                }
            }
        },
        "Encrypt": {
            "$type": "DomainDef",
            "ImpactPerValue": {
                "$type": "ChangeEntityStat",
                "CustomName":"Encrypt",
                "Change": 1,
                "StatRef": "/Stats/Security"
            }
        }
    },
