                "$type": "ResetEntityStats",
                "StatsRef":[
                    "/Stats/Reveal",
                    "/Stats/Security",
                    "/Stats/Defence"
                ]
            }
        }
    ],
    "Domains": {
        "Develop": {
            "$type": "DomainDef",
