                "StatRef": "/Stats/Prosperity"
            }
        },
        "Hurt": {
            "$type": "DomainDef",
            "ImpactPerValue": {
                "$type": "DoIf",
                "CustomName":"Hurt",
                "Predicate": {
                    "$type": "Compare",
                    "More": {
                        "$type": "FromCalcer",
                        "Calcer": {
                            "$type":"PassedValue"
                        }
                    },
                    "Less": {
                        "$type": "StatCalcer",
                        "Stat": "/Stats/Defence"
                    }
                },
                "Impact": {
                    "$type": "ChangeEntityStat",
                    "StatRef": "/Stats/Prosperity",
                    "Change": -1
                }
            }
        },
        "Spy": {
            "$type": "DomainDef",
            "ImpactPerValue": {
                "$type": "DoIf",
                "CustomName":"Spy",
                "Predicate": {
                    "$type": "Compare",
                    "More": {
                        "$type": "FromCalcer",
                        "Calcer": {
                            "$type":"PassedValue"
                        }
                    },
                    "Less": {
                        "$type": "StatCalcer",
                        "Stat": "/Stats/Security"
                    }
                },
                "Impact": {
                    "$type": "ChangeEntityStat",
                    "StatRef": "/Stats/Reveal",
                    "Change": 1,
                    "StatKeyEntity":{
                        "$type":"FromSelector"
                    }
                }
            }
        }
